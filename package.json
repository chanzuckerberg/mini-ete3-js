{
  "name": "phylo-js",
  "version": "0.0.1",
  "description": "A partial port of ETE3 toolkit to typescript to enable phylogenetics in the browser",
  "keywords": [
    "phylogenetics",
    "trees"
  ],
  "browser": "phylo-js.html",
  "scripts": {
    "lint": "npm run type-check && npm run lint-code && npm run lint-formatting",
    "lint-code": "eslint **/*.{ts,js} --fix",
    "lint-code-ci": "eslint **/*.{ts,js}",
    "lint-formatting": "prettier --write .",
    "lint-formatting-ci": "prettier --check .",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/mini-ete3-js.git"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.1.2",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "author": "Sidney Bell",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/chanzuckerberg/mini-ete3-js/issues"
  },
  "homepage": "https://github.com/chanzuckerberg/mini-ete3-js#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
